<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.avci.coinhouse">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/coinhouse"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/coinhouse_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BerkeCanAvci_HW2">
        <service
            android:name=".BinanceService"
            android:exported="false"></service>
        <activity android:name=".InitialActivity"></activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FeedHolderActivity" />
        <activity android:name=".NewsActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>